{"ast":null,"code":"import { mapState, mapMutations } from \"vuex\";\nimport DollarFormatInput from \"@/components/misc/DollarFormatInput.vue\";\nimport StockMutualCdAssetRow from \"./assets/StockMutualCdAssetRow.vue\";\nimport RealEstateAssetRow from \"./assets/RealEstateAssetRow.vue\";\nimport BusinessAssetRow from \"./assets/BusinessAssetRow.vue\";\nexport default {\n  components: {\n    DollarFormatInput,\n    StockMutualCdAssetRow,\n    RealEstateAssetRow,\n    BusinessAssetRow\n  },\n  computed: {\n    ...mapState(\"assets\", [\"savings\"]),\n    ...mapState(\"investments\", [\"stocks\", \"realEstate\", \"businesses\"])\n  },\n  methods: {\n    ...mapMutations(\"assets\", [\"changeSavings\"])\n  }\n};","map":{"version":3,"names":["mapState","mapMutations","DollarFormatInput","StockMutualCdAssetRow","RealEstateAssetRow","BusinessAssetRow","components","computed","methods"],"sources":["src/components/ratrace/balancesheet/BalanceSheetAssets.vue"],"sourcesContent":["<template>\r\n  <div id=\"assets\">\r\n    <h3 class=\"column-title-bar\">Assets</h3>\r\n    <div id=\"asset-list\">\r\n      <div class=\"line-input\">\r\n        <label for=\"savings\">Savings:</label>\r\n        <dollar-format-input :value=\"savings\" @input=\"changeSavings($event)\" />\r\n      </div>\r\n      <div class=\"line-input all-text\">\r\n        <div>Stocks/Mutual's/CDs</div>\r\n        <div>No. of Shares:</div>\r\n        <div>Cost/Share:</div>\r\n      </div>\r\n      <stock-mutual-cd-asset-row v-for=\"(_, index) in stocks\" :key=\"'stocks-' + index\" :index=\"index\" />\r\n      <div class=\"line-input all-text\">\r\n        <div>Real Estate:</div>\r\n        <div>Down Pay:</div>\r\n        <div>Cost:</div>\r\n      </div>\r\n      <real-estate-asset-row v-for=\"(_, index) in realEstate\" :key=\"'real-assets-' + index\" :index=\"index\" />\r\n      <div class=\"line-input all-text\">\r\n        <div>Business:</div>\r\n        <div>Down Pay:</div>\r\n        <div>Cost:</div>\r\n      </div>\r\n      <business-asset-row v-for=\"(_, index) in businesses\" :key=\"'business-assets-' + index\" :index=\"index\" />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState, mapMutations } from \"vuex\";\r\nimport DollarFormatInput from \"@/components/misc/DollarFormatInput.vue\";\r\nimport StockMutualCdAssetRow from \"./assets/StockMutualCdAssetRow.vue\";\r\nimport RealEstateAssetRow from \"./assets/RealEstateAssetRow.vue\";\r\nimport BusinessAssetRow from \"./assets/BusinessAssetRow.vue\";\r\n\r\nexport default {\r\n  components: { DollarFormatInput, StockMutualCdAssetRow, RealEstateAssetRow, BusinessAssetRow },\r\n  computed: {\r\n    ...mapState(\"assets\", [\"savings\"]),\r\n    ...mapState(\"investments\", [\"stocks\", \"realEstate\", \"businesses\"])\r\n  },\r\n  methods: {\r\n    ...mapMutations(\"assets\", [\"changeSavings\"])\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n#assets {\r\n  grid-area: assets;\r\n}\r\n\r\n#asset-list {\r\n  display: flex;\r\n  flex-wrap: nowrap;\r\n  flex-direction: column;\r\n  justify-content: flex-start;\r\n}\r\n\r\n#asset-list > * {\r\n  /* In relation to its parent */\r\n  flex: 1 1 auto;\r\n  margin: 7px;\r\n  /* in relation to its children */\r\n  display: flex;\r\n  flex-wrap: nowrap;\r\n  flex-direction: row;\r\n}\r\n#asset-list > * > * {\r\n  flex: 1 1 auto;\r\n  white-space: nowrap;\r\n}\r\n</style>\r\n"],"mappings":"AA+BA,SAAAA,QAAA,EAAAC,YAAA;AACA,OAAAC,iBAAA;AACA,OAAAC,qBAAA;AACA,OAAAC,kBAAA;AACA,OAAAC,gBAAA;AAEA;EACAC,UAAA;IAAAJ,iBAAA;IAAAC,qBAAA;IAAAC,kBAAA;IAAAC;EAAA;EACAE,QAAA;IACA,GAAAP,QAAA;IACA,GAAAA,QAAA;EACA;EACAQ,OAAA;IACA,GAAAP,YAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}